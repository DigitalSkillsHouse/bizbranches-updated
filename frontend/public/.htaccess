# SPA fallback: serve index.html for listing detail URLs (e.g. /digital-skills-house)
# so the client router can load the business detail page. Required for static export.
<IfModule mod_rewrite.c>
  RewriteEngine On
  RewriteBase /
  # If the request is not for an existing file or directory, serve index.html
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteRule ^ index.html [L]
</IfModule>
